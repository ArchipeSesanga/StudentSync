<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="format-detection" content="telephone=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="author" content="">
    <meta name="keywords" content="">
    <meta name="description" content="">
    <link rel="stylesheet" type="text/css" href="~/css/normalize.css">
    <link rel="stylesheet" type="text/css" href="~/icomoon/icomoon.css">
    <link rel="stylesheet" type="text/css" href="~/css/vendor.css">
    <title>@ViewData["Title"] - StudentSync</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/consumer-style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/StudentSync.styles.css" asp-append-version="true" />
</head>
<body>
    <div id="header-wrap">
        <div class="top-content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-6">
                        <div class="social-links">
                            <ul>
                                <li>
                                    <a href="https://www.facebook.com" target="_blank"><i class="icon icon-facebook"></i></a>
                                </li>
                                <li>
                                    <a href="https://www.twiter.com" target="_blank"><i class="icon icon-twitter"></i></a>
                                </li>
                                <li>
                                    <a href="https://www.youtube.com" target="_blank"><i class="icon icon-youtube-play"></i></a>
                                </li>
                                <li>
                                    <a asp-area="" asp-controller="" asp-action=""><i class="icon icon-behance-square"></i></a>
                                </li>
                            </ul>
                        </div><!--social-links-->
                    </div>
                    <div class="col-md-6">
                        <div class="right-element">
                            <ul>
                                <li>
                                    <partial name="_LoginPartial" />
                                </li>
                            </ul>

                            <a asp-area="" asp-controller="" asp-action="" class="cart for-buy">
                                <i class="icon icon-clipboard"></i>
                                <span>
                                    Cart:(0
                                    $)
                                </span>
                            </a>


                            <div class="action-menu">

                                <div class="search-bar">
                                    <a asp-area="" asp-controller="" asp-action="" class="search-button search-toggle" data-selector="#header-wrap">
                                        <i class="icon icon-search"></i>
                                    </a>
                                    <form role="search" method="get" class="search-box">
                                        <input class="search-field text search-input" placeholder="Search"
                                               type="search">
                                    </form>
                                </div>
                            </div>

                        </div><!--top-right-->
                    </div>

                </div>
            </div>
        </div><!--top-content-->


    </div><!--header-wrap-->
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>

    </div>
    <footer id="footer">
        <div class="container">
            <div class="row">

                <div class="col-md-4">

                    <div class="footer-item">
                        <div class="company-brand">
                            <img img src="/images/main-logo.png" alt="logo" class="footer-logo">
                            <p>
                                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sagittis sed ptibus liberolectus
                                nonet psryroin. Amet sed lorem posuere sit iaculis amet, ac urna. Adipiscing fames
                                semper erat ac in suspendisse iaculis.
                            </p>
                        </div>
                    </div>

                </div>

                <div class="col-md-2">

                    <div class="footer-menu">
                        <h5>About Us</h5>
                        <ul class="menu-list">
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">vision</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">articles </a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">careers</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">service terms</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">donate</a>
                            </li>
                        </ul>
                    </div>

                </div>
                <div class="col-md-2">

                    <div class="footer-menu">
                        <h5>Discover</h5>
                        <ul class="menu-list">
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Home</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Books</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Authors</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Subjects</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Advanced Search</a>
                            </li>
                        </ul>
                    </div>

                </div>
                <div class="col-md-2">

                    <div class="footer-menu">
                        <h5>My account</h5>
                        <ul class="menu-list">
                            <li class="menu-item">
                                <a asp-area="" asp-controller="Consumer" asp-action="Login">Sign In</a>
                            </li>
                            <li class="menu-item">
                                <form id="logoutForm" class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                                    <button id="logout" type="submit" class="nav-link btn btn-link text-dark border border-black border-3 p-3">Logout</button>
                                </form>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">View Cart</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">My Wishtlist</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Track My Order</a>
                            </li>
                        </ul>
                    </div>

                </div>
                <div class="col-md-2">

                    <div class="footer-menu">
                        <h5>Help</h5>
                        <ul class="menu-list">
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Help center</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Report a problem</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Suggesting edits</a>
                            </li>
                            <li class="menu-item">
                                <a asp-area="" asp-controller="" asp-action="">Contact us</a>
                            </li>
                        </ul>
                    </div>

                </div>

            </div>
            <!-- / row -->

        </div>
    </footer>

    <div id="footer-bottom">
        <div class="container">
            <div class="row">
                <div class="col-md-12">

                    <div class="copyright">
                        <div class="row">

                            <div class="col-md-6">
                                <p>
                                    © 2025 All rights reserved. StudentSync <a href="https://www.templatesjungle.com/" target="_blank"></a>
                                </p>
                            </div>

                            <div class="col-md-6">
                                <div class="social-links align-right">
                                    <ul>
                                        <li>
                                            <a href="https://www.facebook.com" target="_blank"><i class="icon icon-facebook"></i></a>
                                        </li>
                                        <li>
                                            <a href="https://www.twiter.com" target="_blank"><i class="icon icon-twitter"></i></a>
                                        </li>
                                        <li>
                                            <a href="https://www.youtube.com.com" target="_blank"><i class="icon icon-youtube-play"></i></a>
                                        </li>
                                        <li>
                                            <a asp-area="" asp-controller="" asp-action=""><i class="icon icon-behance-square"></i></a>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                        </div>
                    </div><!--grid-->

                </div><!--footer-bottom-content-->
            </div>
        </div>
    </div>

    <script src="js/jquery-1.11.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
            crossorigin="anonymous"></script>
    <script src="js/plugins.js"></script>
    <script src="js/script.js"></script>


    <script img src="/lib/jquery/dist/jquery.min.js"></script>
    <script img src="/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script img src="/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>